{% extends 'base.html.twig' %}

{% block title %}Log in!{% endblock %}

{% block body %}
<div class="container-fluid account">
    <div class="img-account">
        <img src="{{ asset('/images/bkg5.jpg') }}" alt="image-accueil" class="img-fluid img-container">
    </div>
    <div class="ico-form">
        <div class="container container-colored no-arrow pt-5 pb-5 text-center">
            <h1 class="text-center">Bienvenue dans votre compte {{last_username}}!</h1>
            <a href="{{ path('my_announces') }}" class="btn btn-primary btn-lg mt-5">Mes annonces</a>
        </div>
    </div>
</div>
{% endblock %}
